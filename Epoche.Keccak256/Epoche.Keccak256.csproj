<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>

		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Authors>Josh Lang</Authors>
		<Description>Keccak256 Hash Calculator (Ethereum's Digest Algorithm)</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/joshlang/Epoche.Keccak256</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Keccak256; Ethereum</PackageTags>
		<PackageId>Keccak256</PackageId>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/joshlang/Epoche.Keccak256</PackageProjectUrl>
		<Version>1.0.0</Version>
		
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
